<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="Model">
  <class catalog="flyaway" name="Country" table="country">
    <id name="countryCode" type="java.lang.String">
      <column length="2" name="countrycode"/>
      <generator class="assigned"/>
    </id>
    <property name="regionCode" type="java.lang.Integer">
      <column name="regioncode" not-null="true"/>
    </property>
    <property name="name" type="java.lang.String">
      <column length="50" name="name" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
